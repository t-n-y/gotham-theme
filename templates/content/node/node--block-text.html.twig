<section class="intro section-padding">
    <div class="container">
        <div class="row">
          {% for item in content.field_block_text['#items'] %}
             <div class="col-md-4 intro-feature">
                  <div class="intro-icon">
                      <span data-icon="&#xe033;" class="icon"></span>
                  </div>
                  <div class="intro-content">
                      <h5>{{item.fieldCollectionItem.field_title.value}}</h5>
                      <p> {{item.fieldCollectionItem.field_text.value}}</p>
                  </div>
              </div>
          {% endfor %}
        </div>
    </div>
</section>