<section class="features-extra section-padding" id="assets">
    <div class="container">
        <div class="row">
            <div class="col-md-5">
                <div class="feature-list">
                    <h3>{{label}}</h3>
                    {{content.body.0['#text']|raw}}
                    <a href="{{content.field_link.0['#url']}}" class="btn btn-ghost btn-accent btn-small">{{content.field_link.0['#title']}}</a>
                </div>
            </div>
        </div>
    </div>
    <div class="macbook-wrap wp3"></div>
    <div class="responsive-feature-img"><img src="{{content.field_image.0['#item'].entity.uri.value}}" alt="responsive devices" alt="{{content.field_image.0['#item'].alt}}"></div>
</section>